#!/bin/bash

syncdoc() {
  rsync -azv --delete --include="*" /home/edgard/Documents/ $1:/home/edgard/Documents/;
}

syncrepos() {
    for dir in $(find /home/edgard/Documents/code -name ".git" -type d -exec dirname {} \;)
    do
      echo "Syncing $dir..."
      (cd $dir && git pull && git push)
    done
}
